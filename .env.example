# Freshwater Vault Environment Variables
# Copy this to .env and fill in your values

# Gemini API Key (for AI Explain feature)
GEMINI_API_KEY=your_gemini_api_key_here

# SendGrid API Key (for email notifications, optional)
SENDGRID_API_KEY=your_sendgrid_key_here
SENDGRID_FROM_EMAIL=notifications@freshwatervault.com

# Analytics admin key
ANALYTICS_KEY=your_secret_admin_key
